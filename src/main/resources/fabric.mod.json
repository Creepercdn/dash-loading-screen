{
  "schemaVersion": 1,
  "id": "dash-loading-screen",
  "version": "${version}",
  "name": "Dash Loading Screen",
  "description": "A simple mod that brings the splash screen of Dashloader to the Minecraft loading screen.",
  "authors": [
    "Creepercdn"
  ],
  "contact": {
    "sources": "https://github.com/Creepercdn/Dash-Loading-Screen",
    "issues": "https://github.com/Creepercdn/Dash-Loading-Screen/issues"
  },
  "license": "LGPL 3.0",
  "environment": "client",
  "entrypoints": {
    "client": [
      "sh.creepercdn.dashloadingscreen.DashLoadingScreenMod"
    ],
    "modmenu": [
      "sh.creepercdn.dashloadingscreen.ModMenuIntegration"
    ]
  },
  "mixins": [
    "dash-loading-screen.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.13",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "fabric-api": "*"
  },
  "icon": "assets/dashloadingscreen/icon.png"
}